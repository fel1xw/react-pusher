  "repository": {
    "type": "git",
    "url": "git+https://github.com/fel1xw/react-pusher-hoc.git"
  },
  "keywords": [
    "react",
    "pusher",
    "hoc",
    "pusher-js",
    "higher-order component"
  ],
  "author": {
    "name": "Felix Wostal",
    "url": "https://felixwostal.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fel1xw/react-pusher-hoc/issues"
  },
  "homepage": "https://github.com/fel1xw/react-pusher-hoc#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "size-limit": "^0.21.0",
    "ts-jest": "^23.10.5",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.1"
  },
  "peerDependencies": {
    "react": ">=16.3"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
